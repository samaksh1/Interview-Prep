<%- include('partials/header'); -%>
<link rel="stylesheet" href="/itemspage.css">
<div>
  <h1>  Experiences </h1>

 <div class="X">
   <div class = "wrapper">
  <% if (exps.length > 0) { %>
        <% exps.forEach(exp => { %>
          <div class = "SG">
            <li>
          <div >
          <div class="card mb-4 box-shadow">
            <img class="card-img-top" src="<%= exp.displayPicPath %>" alt="Card image cap">
            <div class="card-body">
              <h5 class="card-title"><%= exp.name %></h5>
              <p class="card-text"><%= exp.smalldesc %><a href ="#">read more</a></p>
              <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
            </div>
            </div>
            </div>
            </li>
            </div>
 <% }) %>
<% } else { %>
 <p>There are no exps to display...</p>
<% } %>
</div>
<%- include('partials/footer'); -%>
</div>

</div>
