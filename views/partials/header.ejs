            <li class="nav-item">
              <a class="nav-link" href="/company/add_exp">Share Your Exp</a>
            </li>
            <% }}%>
            <% if (title) { if (title=="Topics List") { %>
            <li class="nav-item">
              <a class="nav-link" href="/topic/add_ques">Add a new ques</a>
            </li>
            <% }}%>
      <li class="nav-item">
        <a class="nav-link" href="/about">About</a>
      </li>
    </ul>
  </div>
</nav>
